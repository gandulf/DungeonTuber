msgstr "{0} bearbeitet."

# Valence
msgid "Valence"
msgstr "Valenz"

msgid "Valence Description"
msgstr "Die Wertigkeit bzw. die Ausprägung von angenehmen (positiven) oder unangenehmen (negativen) Empfindungen."

msgid "Valence Low"
msgstr "Negativ (z. B. traurig, wütend)"

msgid "Valence Medium"
msgstr "Neutral"

msgid "Valence High"
msgstr "Positiv (z. B. glücklich, freudig)"


# Arousal
msgid "Arousal"
msgstr "Erregung"

msgid "Arousal Description"
msgstr "Der Grad der psychischen und physischen Aktivierung, von schläfrig bis hin zu hochgradig aufgeregt."

msgid "Arousal Low"
msgstr "Niedrig (z. B. entspannt, schläfrig)"

msgid "Arousal Medium"
msgstr "Mittel"

msgid "Arousal High"
msgstr "Hoch (z. B. aufgeregt, intensiv)"

# Tempo
msgid "Tempo"
msgstr "Tempo"

msgid "Tempo Description"
msgstr "Tempo / Bewegung & Intensität"

msgid "Tempo Low"
msgstr "Ruhig, dezent und sehr langsam (getragen)"

msgid "Tempo Medium"
msgstr "Moderat (fließend, neutral)"

msgid "Tempo High"
msgstr "Antreibend, schnell und überwältigend (episch, hektisch, treibend)"

# Dunkelheit (Darkness)
msgid "Darkness"
msgstr "Dunkelheit"

msgid "Darkness Description"
msgstr "Bedrohung & Licht"

msgid "Darkness Low"
msgstr "Hell, freundlich und einladend"

msgid "Darkness Medium"
msgstr "Neutral (unvoreingenommen, beobachtend)"

msgid "Darkness High"
msgstr "Finster, bedrohlich und albtraumhaft (Dämonisches, Intrigen)"

# Emotional
msgid "Emotional"
msgstr "Emotional"

msgid "Emotional Description"
msgstr "Emotionale Wärme & Distanz"

msgid "Emotional Low"
msgstr "Kühl, distanziert und sachlich"

msgid "Emotional Medium"
msgstr "Ausgeglichen (ruhig erzählend)"

msgid "Emotional High"
msgstr "Sehr emotional, herzergreifend und intim (Tragik, Abschiede)"

# Mystik (Mysticism)
msgid "Mysticism"
msgstr "Mystik"

msgid "Mysticism Description"
msgstr "Übernatürlichkeit vs. Weltlichkeit"

msgid "Mysticism Low"
msgstr "Weltlich, bodenständig und irdisch, Tavernentauglich, volksnah und handgemacht (Reise, Alltag)"

msgid "Mysticism Medium"
msgstr "Gelegentliche magische Nuancen, Zivilisiert (klassisch-orchestral, geordnet)"

msgid "Mysticism High"
msgstr "Stark magisch, transzendent, abstrakt, sphärisch entfremdet und fremdartig (Visionen, Götter)"

# Spannung (Tension)
msgid "Tension"
msgstr "Spannung"

msgid "Tension Description"
msgstr "Erwartung & Entspannung"

msgid "Tension Low"
msgstr "Entspannend, abgeschlossen und sicher"

msgid "Tension Medium"
msgstr "Beiläufig (plätschernd, unaufgeregt)"

msgid "Tension High"
msgstr "Hochspannend, nervös und lauernd (kurz vor dem Kampf)"

# Heroik (Heroism)
msgid "Heroism"
msgstr "Heroik"

msgid "Heroism Description"
msgstr "Größe & Schlichtheit"

msgid "Heroism Low"
msgstr "Schlicht, persönlich, intim und klein (Kneipe, Alltag)"

msgid "Heroism Medium"
msgstr "Solidarisch (gefestigt, mutig)"

msgid "Heroism High"
msgstr "Heroisch, legendär, schicksalhaft und göttlich (Schlachten, Siege)"



